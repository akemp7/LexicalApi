@using LexicalClient.Models;
@model List<Word>;
<h1>All Words:</h1>

<ol>
@foreach (Word word in Model)
{
    <ul>
        <li>@Html.ActionLink(@word.name, "Test", new Word {wordId = word.wordId, name = word.name, rating = word.rating, ratingCount = word.ratingCount })</li>
        
    </ul>
    <br>
}
</ol>

<h3>@Html.ActionLink("Add a word", "Rating")</h3>